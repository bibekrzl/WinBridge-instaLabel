{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "PrintServer": {
    "DefaultLabelWidth": 100,
    "DefaultLabelHeight": 50,
    "MaxImageSize": 10485760,
    "SupportedImageFormats": ["PNG", "JPG", "JPEG", "BMP", "GIF"],
    "AutoDiscoverPrinters": true,
    "PrinterScanInterval": 30000
  }
} 